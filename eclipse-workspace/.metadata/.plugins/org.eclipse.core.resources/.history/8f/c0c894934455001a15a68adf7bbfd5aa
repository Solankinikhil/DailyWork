package com.cts.training5;

public class ThreadOne implements Runnable
{   
	@Override
	public void run() 
	{
		for(int i = 1; i<50;i++)
		{
			if(i%2 != 0) {
			System.out.println("Thread-1:"+i);
			}
			try {
				Thread.sleep(2000);
			} catch (InterruptedException e) {
			e.printStackTrace();
			}
		}
	}

	
}
